{
  "name": "express-handler-errors",
  "version": "2.3.1",
  "node": "v12.14.1",
  "description": "Middleware to handle erros on express framework",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit --verbose",
    "coverage": "NODE_ENV=test rimraf coverage && jest --coverage --silent --detectOpenHandles --forceExit",
    "tsc": "tsc",
    "lint": "eslint src --ext ts",
    "build": "rimraf dist && npm run tsc",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json",
            "yarn.lock"
          ]
        }
      ]
    ]
  },
  "keywords": [
    "express",
    "errors",
    "error-handler"
  ],
  "author": "Vitor Delfino",
  "homepage": "https://github.com/vitordelfino/express-handler-errors",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitordelfino/express-handler-errors.git"
  },
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "rimraf": "^3.0.2",
    "winston": "^3.2.1",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/express": "^4.17.7",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/supertest": "^2.0.10",
    "@types/winston": "^2.4.4",
    "@types/yup": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "class-transformer": "^0.2.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "semantic-release": "^17.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
